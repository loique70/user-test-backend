generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Users {
    id Int @id @default(autoincrement())
    lastName String 
    firstName String 
    userName String
    password String
    email String @unique
    isActive Boolean
    activationLink String @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    deleted Boolean @default(false)
}
